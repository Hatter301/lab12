#include <iostream>
using namespace std;
int Quarter(double x, double y) {
	if ((x > 0) and (y > 0))
		cout << 1<<" ";
	else if ((x < 0) and (y > 0))
		cout << 2<<" ";
	else if ((x < 0) and (y < 0))
		cout << 3<<" ";
	else cout << 4<<" ";
	return 0;
}
int i = 0;
double x,y;
int main() {
	while (i != 3) {
		cin >> x>>y;
		if ((x==0) or (y==0))
			cout << "Данные введены некорректно ";
		else {
			Quarter(x, y);
			i++;
		}
	}
}
